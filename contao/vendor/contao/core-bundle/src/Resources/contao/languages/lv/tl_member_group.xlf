      <trans-unit id="tl_member_group.name.0">
        <source>Title</source>
        <target>Virsraksts</target>
      </trans-unit>
      <trans-unit id="tl_member_group.name.1">
        <source>Please enter the group title.</source>
        <target>Ievadiet grupas nosaukumu.</target>
      </trans-unit>
      <trans-unit id="tl_member_group.redirect.0">
        <source>Redirect on login</source>
        <target>Virzienmaiņa pieslēdzoties</target>
      </trans-unit>
      <trans-unit id="tl_member_group.redirect.1">
        <source>Redirect group members to a custom page when they log in.</source>
        <target>Novirzīt grupas biedrus uz pielāgotu lapu, kad tie pieslēdzas.</target>
      </trans-unit>
      <trans-unit id="tl_member_group.jumpTo.0">
        <source>Redirect page</source>
        <target>Pārsūtīšanas lapa</target>
      </trans-unit>
